{
  "expo": {
    "name": "chatbot",
    "slug": "chatbot",
    "description": "This is a ChatAI app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon.ico",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fffcc9"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.ico",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.ico"
    }
  }
}